  "homepage": "https://OlegWeremyew.github.io/MyPortfolio",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.0.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "formic": "^0.0.10",
    "react": "^18.0.0",
    "react-dom": "^17.0.2",
    "react-parallax-tilt": "^1.7.43",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-tilt": "^0.1.4",
    "react-typing-effect": "^2.0.5",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4"
  },
  "scripts": {
    "start:dev": "NODE_ENV=development react-scripts start",
    "start:prod": "NODE_ENV=production react-scripts start",
    "build:dev": "NODE_ENV=development react-scripts build",
    "build:prod": "NODE_ENV=production react-scripts build",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules && yarn cache clean",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn cache clean",
    "reset_and_install": "rm -rf node_modules && rm yarn.lock && yarn cache clean && yarn",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "lint:css:fix": "stylelint '**/*.{css,scss}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,json,md}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
